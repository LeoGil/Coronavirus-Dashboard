  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "gh-pages": "^3.0.0",
    "sucrase": "^3.15.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.47",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  }
